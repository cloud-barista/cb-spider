#!/bin/bash

source ../header.sh




echo -e "\n============ STEP-3: DELETE CONNECT CONFIG (AWS:OHIO)"

source ./setup.env

# for Cloud Driver Info
time $CLIPATH/spctl -u "$API_USERNAME" -p "$API_PASSWORD" driver unregister --DriverName aws-driver01

 # for Cloud Credential Info
time $CLIPATH/spctl -u "$API_USERNAME" -p "$API_PASSWORD" credential unregister --CredentialName aws-credential01

 # for Cloud Region Info
time $CLIPATH/spctl -u "$API_USERNAME" -p "$API_PASSWORD" region unregister --RegionName aws-ohio

 # for Cloud Connection Config Info
time $CLIPATH/spctl -u "$API_USERNAME" -p "$API_PASSWORD" connection delete --ConfigName aws-ohio-config





source ../tailer.sh
